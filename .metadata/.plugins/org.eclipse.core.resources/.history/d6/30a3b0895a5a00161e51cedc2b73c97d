@Component
public class Appl {

	@Autowired
	private DataSource datasource;

	public Appl(){
		System.out.println("HIHI");
	}
	
	public static void main(String[] args){
		ApplicationContext context = new ClassPathXmlApplicationContext("bean.xml");
	}
}
